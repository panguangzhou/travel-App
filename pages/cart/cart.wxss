page{
  background: #F5F5F5;
}
.cart_box{
  width: 100%;
  height: 100px;
  margin-top:20px;
  text-align: center;
}
.cart_box .p{
  display: block;
  margin:10px 0;
  font-size: 12px;
  color:#808080;
}
.cart_box .btn{
  font-size: 12px;
  width: 100px;
  height: 20px;
  line-height: 20px;
  border: 1px solid red;
  background: #fff;
  color:red;
  border-radius: 0;
}
/* 下面技术支持板块 */
.support{
  width: 100%;
  height: 100px;
  display: flex;
  position: absolute;
  bottom:-100px;
  left: 0;
}
.support .support_box{
  width: 120px;
  margin: auto;
  font-size: 12px;
  color:yellow;
}